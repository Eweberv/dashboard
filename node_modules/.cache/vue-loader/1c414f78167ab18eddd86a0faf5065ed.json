egister',\n        data: () => ({\n            name: null,\n            password: null,\n            email: null,\n            snackbar: false,\n            color: null,\n            snackbarText: null,\n        }),\n        methods: {\n            register() {\n\n                axios.post(`http://localhost:3000/register`, {\n                    name: this.name,\n                    password: this.password,\n                    email: this.email\n                }).then((res) => {\n                    this.color = \"success\";\n                    this.snackbarText = res.data;\n                    this.snackbar = true;\n                }).catch(error => {\n                    this.snackbarText = error.response.data.err;\n                    this.color = \"error\";\n                    this.snackbar = true;\n\n                });\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}