\n                            <v-form>\n                                <v-text-field\n                                        v-model=\"loginStr\"\n                                        label=\"Login\"\n                                        name=\"login\"\n                                        type=\"text\"\n                                ></v-text-field>\n\n                                <v-text-field\n                                        v-model=\"passwordStr\"\n                                        id=\"password\"\n                                        label=\"Password\"\n                                        name=\"password\"\n                                        type=\"password\"\n                                ></v-text-field>\n                            </v-form>\n                        </v-card-text>\n                        <v-card-actions>\n                            <router-link to=\"/Register\">No account? Register now</router-link>\n                            <v-spacer></v-spacer>\n                            <v-btn color=\"primary\" @click=\"login()\">Login</v-btn>\n                        </v-card-actions>\n                        <p>If you have trouble loggin in: <router-link to=\"/home\">direct link to my Services</router-link></p>\n                    </v-card>\n                </v-flex>\n            </v-layout>\n        </v-container>\n    </div>\n</template>\n\n<script>\n    import axios from \"axios\";\n    import router from '../router/router'\n\n    export default {\n        name: 'Login',\n        data: () => ({\n           loginStr: null,\n           passwordStr: null\n        }),\n        methods: {\n            login() {\n                axios.post(`http://localhost:3000/login`, {\n                    login: this.loginStr,\n                    password: this.passwordStr\n                }).then((res) => {\n                    console.log(res.data);\n                    if (res.data.message === \"logged in\") {\n                    router.push('/home')\n                    }\n                }).catch(function (error) {\n                    console.log(\"Error response from API: \", error.message);\n                });\n            }\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>"]}]}