list\">\n            Shopping List\n        </div>\n        <div>\n            <p>Total cost: {{totalCost()}}</p>\n            <select v-model=\"selected\">\n                <option value=\"\">Choose a fruit</option>\n                <option v-for=\"(fruit, index) in fruits\" :key=\"index\" v-bind:value=\"fruit.name\">{{fruit.name}}</option>\n            </select>\n            <button v-on:click=\"addToCard()\">Add to card</button>\n            <ul>\n                <li v-for=\"(elem, index) in card\" v-bind:key=\"index\">{{elem}}</li>\n            </ul>\n        </div>\n    </div>\n</template>\n\n<script>\n    export default {\n        name: \"Shopping list\",\n        data: () => ({\n            fruits: [\n                {\n                    name: 'banane',\n                    cost: 5\n                },\n                {\n                    name: 'pomme',\n                    cost: 10\n                },\n                {\n                    name: 'ananas',\n                    cost: 15\n                }\n            ],\n            selected: null,\n            card: [],\n        }),\n        methods: {\n            totalCost() {\n                let total = 0;\n                for (let fruit of this.fruits) {\n                    total += fruit.cost\n                }\n                return (total);\n            },\n            addToCard() {\n                this.card.push(this.selected)\n            }\n        },\n//        mounted() {\n        //        this.totalCost();\n        //    }\n    }\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/components"}]}