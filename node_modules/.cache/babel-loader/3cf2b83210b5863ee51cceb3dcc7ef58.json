A;;AACA,UAAA,KAAA,cAAA,KAAA,KAAA,MAAA,CAAA,OAAA,EAAA;AACA,aAAA,cAAA,GAAA,KAAA,MAAA,CAAA,UAAA;AACA,aAAA,WAAA,CAAA,WAAA,GAAA,CAAA,KAAA,WAAA,CAAA,WAAA,GAAA,CAAA,GAAA,CAAA,GAAA,EAAA,EAAA,OAAA,CAAA,CAAA,CAAA;AACA,aAAA,WAAA,CAAA,iBAAA,GAAA,CAAA,KAAA,WAAA,CAAA,iBAAA,GAAA,CAAA,GAAA,CAAA,GAAA,EAAA,EAAA,OAAA,CAAA,CAAA,CAAA;AACA,OAJA,MAIA;AACA,aAAA,cAAA,GAAA,KAAA,MAAA,CAAA,OAAA;AACA,aAAA,WAAA,CAAA,WAAA,GAAA,CAAA,CAAA,KAAA,WAAA,CAAA,WAAA,GAAA,EAAA,IAAA,CAAA,GAAA,CAAA,EAAA,OAAA,CAAA,CAAA,CAAA;AACA,aAAA,WAAA,CAAA,iBAAA,GAAA,CAAA,CAAA,KAAA,WAAA,CAAA,iBAAA,GAAA,EAAA,IAAA,CAAA,GAAA,CAAA,EAAA,OAAA,CAAA,CAAA,CAAA;AACA;AACA,KAZA;AAaA,IAAA,YAbA,0BAaA;AACA,WAAA,OAAA,CAAA,iBAAA,CAAA,KAAA,QAAA;AACA,KAfA;AAgBA,IAAA,QAhBA,sBAgBA;AAAA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,IAAA;;AACA,UAAA,KAAA,IAAA,KAAA,IAAA,IAAA,KAAA,IAAA,KAAA,EAAA,EAAA;AACA,aAAA,WAAA,CAAA,MAAA,GAAA,IAAA;AACA,aAAA,QAAA,GAAA,mBAAA;AACA,eAAA,CAAA,CAAA;AACA;;AACA,WAAA,QAAA,GAAA,IAAA,CAPA,CASA;;AACA,WAAA,cAAA,GAAA,KAAA,MAAA,CAAA,UAAA;AAEA,MAAA,KAAA,CAAA,GAAA,sDAAA,KAAA,IAAA,GAAA,EAAA,EACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,GAAA,EAAA;AACA,UAAA,KAAA,CAAA,WAAA,CAAA,MAAA,GAAA,IAAA;AACA,UAAA,KAAA,CAAA,QAAA,oBAAA,KAAA,CAAA,IAAA;AACA,iBAAA,CAAA,CAAA;AACA;;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,CAAA,MAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CAAA,CAAA,EAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,WAAA,CAAA,iBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,UAAA;AACA,QAAA,KAAA,CAAA,WAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,QAAA;AACA,QAAA,KAAA,CAAA,WAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAdA,EAcA,KAdA,CAcA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,2BAAA,EAAA,KAAA,CAAA,IAAA;AACA,OAhBA;AAiBA;AA7CA,GAxBA;AAuEA,EAAA,OAvEA,qBAuEA;AACA,SAAA,cAAA,GAAA,KAAA,MAAA,CAAA,UAAA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,gBAAA,KAAA,QAAA;AACA;AA1EA,CAAA","sourcesContent":["<template xmlns:v-slot=\"http://www.w3.org/1999/XSL/Transform\">\n    <v-card\n            max-width=\"500\"\n            class=\"mx-auto card\"\n    >\n        <v-toolbar\n                color=\"pink\"\n                dark\n        >\n            <v-toolbar-title>Meteo</v-toolbar-title>\n\n            <v-spacer></v-spacer>\n\n            <v-btn class=\"mx-2\" fab dark small color=\"primary\" @click=\"removeWidget\">\n                <v-icon dark>mdi-minus</v-icon>\n            </v-btn>\n\n        </v-toolbar>\n\n        <div class=\"content\">\n            <p>Enter a city to display its temperature</p>\n            <div class=\"flexContainer\">\n                <v-text-field class=\"CityInput\" label=\"City\" hide-details=\"auto\" v-model=\"city\"></v-text-field>\n                <v-btn\n                        class=\"ma-2 SearchBtn\"\n                        :loading=false\n                        :disabled=false\n                        color=\"info\"\n                        @click=\"queryApi()\"\n                >\n                    Search\n                    <template v-slot:loader>\n        <span class=\"custom-loader\">\n          <v-icon light>cached</v-icon>\n        </span>\n                    </template>\n                </v-btn>\n            </div>\n\n          <!--  <a href=\"https://imgbb.com/\"><img src=\"https://i.ibb.co/f1TVhTx/fahrenheit.png\" alt=\"fahrenheit\" border=\"0\"></a>\n            <a href=\"https://imgbb.com/\"><img src=\"https://i.ibb.co/pJzD1rN/celsius.png\" alt=\"celsius\" border=\"0\"></a>-->\n\n            <div v-if=\"apiResponse.status !== null\">\n                <div class=\"tempStat\">\n                    <p class=\"subtitle-1\">Temperature: {{apiResponse.temperature}}</p>\n                    <v-img class=\"temperatureTypeLogo\" id=\"temperatureLogo\" :src=\"temperatureImg\" @click=\"swichTempUnit()\"></v-img>\n                </div>\n                <div class=\"otherStats\">\n                    <p>Feeled temperature: {{apiResponse.feeledTemperature}}</p>\n                    <p>Humidity: {{apiResponse.humidity}}</p>\n                    <p>Wind Speed: {{apiResponse.windSpeed}}</p>\n                    <p>Weather: {{apiResponse.weather}}</p>\n                </div>\n            </div>\n            <div>\n            </div>\n            <p v-if=\"apiError !== null\">{{apiError}}</p>\n        </div>\n    </v-card>\n</template>\n\n<script>\n    import axios from 'axios'\n    export default {\n        name: \"SteamServicePlayerNb\",\n        props: {\n            widgetId: null,\n        },\n        data: () => ({\n            apiResponse: {\n                status: null,\n                weather: null,\n                temperature: null,\n                feeledTemperature: null,\n                humidity: null,\n                windSpeed: null,\n            },\n            apiError: null,\n            city: null,\n\n            images: {\n                fahrenheit: 'https://i.ibb.co/f1TVhTx/fahrenheit.png',\n                celsius: 'https://i.ibb.co/pJzD1rN/celsius.png',\n            },\n            temperatureImg: null,\n            fahrenheit: true,\n        }),\n        methods: {\n            swichTempUnit() {\n                console.log(\"switch temperature image\");\n                if (this.temperatureImg === this.images.celsius) {\n                    this.temperatureImg = this.images.fahrenheit;\n                    this.apiResponse.temperature = ((this.apiResponse.temperature * 9/5) + 32).toFixed(2);\n                    this.apiResponse.feeledTemperature = ((this.apiResponse.feeledTemperature * 9/5) + 32).toFixed(2);\n                } else {\n                    this.temperatureImg = this.images.celsius;\n                    this.apiResponse.temperature = ((this.apiResponse.temperature - 32) * 5/9).toFixed(2);\n                    this.apiResponse.feeledTemperature = ((this.apiResponse.feeledTemperature - 32) * 5/9).toFixed(2);\n                }\n            },\n            removeWidget() {\n                this.$parent.removeChildWidget(this.widgetId);\n            },\n            queryApi() {\n                console.log(this.city);\n                if (this.city === null || this.city === '') {\n                    this.apiResponse.status = null;\n                    this.apiError = \"No city specified\";\n                    return -1;\n                }\n                this.apiError = null;\n\n                //reset l'image a fahrenheit quand y'a une nouvelle recherche\n                this.temperatureImg = this.images.fahrenheit;\n\n                axios.get(`http://localhost:3000/getMeteoForCity?city=${this.city}`, {\n                }).then((res) => {\n                    if (res.data === 404) {\n                        this.apiResponse.status = null;\n                        this.apiError = `City \"${this.city}\" not found`;\n                        return (-1);\n                    }\n                    this.apiError = null;\n                    this.apiResponse.status = \"ok\";\n                    this.apiResponse.weather = res.data.weather[0].main;\n                    this.apiResponse.temperature = res.data.main.temp;\n                    this.apiResponse.feeledTemperature = res.data.main.feels_like;\n                    this.apiResponse.humidity = res.data.main.humidity;\n                    this.apiResponse.windSpeed = res.data.wind.speed;\n                }).catch(function (error) {\n                    console.log(\"Error response from API: \", error.data);\n                });\n            }\n        },\n        mounted() {\n            this.temperatureImg = this.images.fahrenheit;\n            console.log(\"widget id: \" + this.widgetId);\n        },\n    }\n</script>\n\n<style scoped>\n    .tempStat {\n        display: flex;\n        max-width: 450px;\n    }\n\n    .card {\n        margin: 30px;\n    }\n\n    .content {\n        padding: 10px;\n    }\n\n    .CityInput {\n        margin-bottom: 16px;\n        max-width: 200px;\n    }\n\n    .SearchBtn {\n        margin:10px;\n        bottom: 0;\n    }\n\n    .flexContainer {\n        display: flex;\n    }\n\n    .temperatureTypeLogo {\n        margin-left: 10px;\n        max-width: 40px;\n        max-height: 40px;\n    }\n\n\n</style>"],"sourceRoot":"src/components"}]}